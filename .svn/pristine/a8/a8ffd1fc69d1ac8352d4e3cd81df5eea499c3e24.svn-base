public class TestApprovenFunciton extends BaseInfo{ 
	//null 
	private int testId; 
	//null 
	private String testName; 
	//null 
	private String testRemark; 
	//当前步骤（1、请假已提交；2、请假已审批通过；3、请假审批不通过） 
	private String step; 
    public TestApprovenFunciton(){
	}
    public TestApprovenFunciton(int testId,String testName,String testRemark,String step){
		this.testId=testId;
		this.testName=testName;
		this.testRemark=testRemark;
		this.step=step;
	}
    public TestApprovenFunciton(String testName,String testRemark,String step){
		this.testName=testName;
		this.testRemark=testRemark;
		this.step=step;
	}
	public void setTestId(int testId){ 
		this.testId=testId;
	}
	public int getTestId(){ 
		return this.testId;
	}
	public void setTestName(String testName){ 
		this.testName=testName;
	}
	public String getTestName(){ 
		return this.testName;
	}
	public void setTestRemark(String testRemark){ 
		this.testRemark=testRemark;
	}
	public String getTestRemark(){ 
		return this.testRemark;
	}
	public void setStep(String step){ 
		this.step=step;
	}
	public String getStep(){ 
		return this.step;
	}
	@Override
	public String toString() {
		return "TestApprovenFunciton[testId=" + testId+",testName=" + testName+",testRemark=" + testRemark+",step=" + step+"]";
	}
}