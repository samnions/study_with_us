<script type="text/javascript">
    $("#xf a,#es a").click(function(){
        var pid = $(this).parent().parent().attr('id');
        $("#"+pid+" a").removeClass('selected');
        $(this).addClass('selected');
        var cate_id = $(this).attr('data-bid');
        if(cate_id){
            $.ajax({
                type: "post",
                url : "/fangcms_ajax_getPosition.html",
                dataType:'json',
                data:{cate_id:cate_id},
                success: function(json){
                    if(json.status==1){
                        getTpl(json.model,json,pid+'_list');
                    }else{
                        msg_show(json.msg,2)
                    }
                }
            });
        }
    });
    $(document).ready(function() {
        $("#slide").hover(function(){
            $("#ui-arrow").show();
        },function(){
            $("#ui-arrow").hide();
        });
        $('#slide').slide({
            effect: 'scroll',
            speed: 'slow',
            navCls: 'slide-nav',
            contentCls: 'slide-con',
            caption: true,
            prevBtnCls:'slide-left',
            nextBtnCls:'slide-right',
            evtype: 'click',
            view:{"width":830,'height':433}
        });
    });
