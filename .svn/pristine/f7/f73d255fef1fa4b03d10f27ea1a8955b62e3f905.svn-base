<?xml version="1.0" encoding="UTF-8" ?> 

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.sd.farmework.mapper.SysDictionaryInfoMapper"> 

	<resultMap type="com.sd.farmework.pojo.SysDictionaryInfo" id="SysDictionaryInfoMap"> 

		<result column="DICTIONARY_ID" property="dictionaryId"/>
		<result column="DICTIONARY_NAME" property="dictionaryName"/>
		<result column="DICTIONARY_DESC" property="dictionaryDesc"/>
		<result column="DICTIONARY_TYPE_ID" property="dictionaryTypeId"/>
		
		<!-- 系统公共属性 必须按照规定存取数据start -->
		<result column="REMARK" property="remark"/>
		<result column="STATUS" property="status"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="CREATE_USER_ID" property="createUserId"/>
		<result column="CREATE_USER_NAME" property="createUserName"/>
		<result column="CREATE_TEAM_ID" property="createTeamId"/>
		<result column="CREATE_TEAM_NAME" property="createTeamName"/>
		<result column="CREATE_DEPART_ID" property="createDepartId"/>
		<result column="CREATE_DEPART_NAME" property="createDepartName"/>
		<result column="CREATE_UNIT_ID" property="createUnitId"/>
		<result column="CREATE_UNIT_Name" property="createUnitName"/>
		<result column="LAST_UPDATE_TIME" property="lastUpdateTime"/>
		<result column="LAST_UPDATE_USER_ID" property="lastUpdateUserId"/>
		<result column="LAST_UPDATE_TEAM_ID" property="lastUpdateTeamId"/>
		<result column="LAST_UPDATE_TEAM_NAME" property="lastUpdateTeamName"/>
		<result column="LAST_UPDATE_DEPART_ID" property="lastUpdateDepartId"/>
		<result column="LAST_UPDATE_DEPART_NAME" property="lastUpdateDepartName"/>
		<result column="LAST_UPDATE_UNIT_ID" property="lastUpdateUnitId"/>
		<result column="LAST_UPDATE_UNIT_Name" property="lastUpdateUnitName"/>
		<!-- 系统公共属性 必须按照规定存取数据end -->

	</resultMap>
	<update id="update" parameterType="com.sd.farmework.pojo.SysDictionaryInfo">
		UPDATE SYS_DICTIONARY_INFO set 
		t.DICTIONARY_ID=#{dictionary_id},
		t.DICTIONARY_NAME=#{dictionary_name},
		t.DICTIONARY_DESC=#{dictionary_desc},
		t.DICTIONARY_TYPE=#{dictionary_type},
		t.CREATE_USER_ID=#{create_user_id},
		t.CREATE_USER_NAME=#{create_user_name},
		t.CREATE_TIME=#{create_time},
		t.UPDATE_USER_ID=#{update_user_id},
		t.UPDATE_USER_NAME=#{update_user_name},
		t.UPDATE_TIME=#{update_time},
		t.STATUS=#{status}
		where 1=1 
	</update>
	<select id="queryById" parameterType="com.sd.farmework.pojo.SysDictionaryInfo"  resultMap="SysDictionaryInfoMap">
		SELECT 
		t.DICTIONARY_ID,
		t.DICTIONARY_NAME,
		t.DICTIONARY_DESC,
		t.DICTIONARY_TYPE,
		t.CREATE_USER_ID,
		t.CREATE_USER_NAME,
		t.CREATE_TIME,
		t.UPDATE_USER_ID,
		t.UPDATE_USER_NAME,
		t.UPDATE_TIME,
		t.STATUS
		FROM  SYS_DICTIONARY_INFO t 
		where  1 =1 
	</select>
	<select id="query" parameterType="com.sd.farmework.pojo.SysDictionaryInfo"  resultMap="SysDictionaryInfoMap">
		SELECT 
		t.DICTIONARY_ID,
		t.DICTIONARY_NAME,
		t.DICTIONARY_DESC,
		t.DICTIONARY_TYPE,
		t.CREATE_USER_ID,
		t.CREATE_USER_NAME,
		t.CREATE_TIME,
		t.UPDATE_USER_ID,
		t.UPDATE_USER_NAME,
		t.UPDATE_TIME,
		t.STATUS
		FROM  SYS_DICTIONARY_INFO t 
		where  1 =1 
	</select>
	<select id="queryListByPage" parameterType="com.sd.farmework.pojo.SysDictionaryInfo"  resultMap="SysDictionaryInfoMap">
		<![CDATA[ 
		SELECT RW,a.*
		FROM (
		SELECT ROWNUM RW,
		t.DICTIONARY_ID,
		t.DICTIONARY_NAME,
		t.DICTIONARY_DESC,
		t.DICTIONARY_TYPE,
		t.CREATE_USER_ID,
		t.CREATE_USER_NAME,
		t.CREATE_TIME,
		t.UPDATE_USER_ID,
		t.UPDATE_USER_NAME,
		t.UPDATE_TIME,
		t.STATUS
		FROM  SYS_DICTIONARY_INFO t 
 		) a 
		WHERE RW between #{pageSize} * (#{currPage} - 1) + 1 AND (#{pageSize} * (#{currPage} - 1)) + #{pageSize}
		]]> 
	</select>
	<select id="queryList" parameterType="com.sd.farmework.pojo.SysDictionaryInfo"  resultMap="SysDictionaryInfoMap">
		SELECT 
		t.DICTIONARY_ID,
		t.DICTIONARY_NAME,
		t.DICTIONARY_DESC,
		t.DICTIONARY_TYPE,
		t.CREATE_USER_ID,
		t.CREATE_USER_NAME,
		t.CREATE_TIME,
		t.UPDATE_USER_ID,
		t.UPDATE_USER_NAME,
		t.UPDATE_TIME,
		t.STATUS
		FROM  SYS_DICTIONARY_INFO t 
	</select>
	<insert id="add">
	insert into SYS_DICTIONARY_INFO
	(
		DICTIONARY_ID,
		DICTIONARY_NAME,
		DICTIONARY_DESC,
		DICTIONARY_TYPE,
		CREATE_USER_ID,
		CREATE_USER_NAME,
		CREATE_TIME,
		UPDATE_USER_ID,
		UPDATE_USER_NAME,
		UPDATE_TIME,
		STATUS
	)
	values
	(
		#{dictionaryId},
		#{dictionaryName},
		#{dictionaryDesc},
		#{dictionaryType},
		#{createUserId},
		#{createUserName},
		#{createTime},
		#{updateUserId},
		#{updateUserName},
		#{updateTime},
		#{status}
	)
	</insert>
	<delete id="delete">
		delete from SYS_DICTIONARY_INFO where 1=2 
	</delete>
	<select id="queryCount"  parameterType="com.sd.farmework.pojo.SysDictionaryInfo"  resultType="java.lang.Integer">
   		select    count(*) from SYS_DICTIONARY_INFO where 1=1
	</select>
</mapper> 

